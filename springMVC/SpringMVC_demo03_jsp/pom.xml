            <version>3.1.0</version>
            <!-- 设置依赖由服务器提供，不打包到war包里的lib文件夹下-->
            <scope>provided</scope>
        </dependency>
        <!--Spring5和Thymeleaf整合包-->
        <dependency>
            <groupId>org.thymeleaf</groupId>
            <artifactId>thymeleaf-spring5</artifactId>
            <version>3.0.12.RELEASE</version>
